@font-face {
  font-family: 'Market Deco';
  src: url(<%= font_url 'Market_Deco.ttf' %>) format('truetype');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'Diner Obese';
  src: url(<%= font_url 'Diner-Obese-webfont.eot' %>);
  src: url(<%= font_url 'Diner-Obese-webfont.eot?#iefix' %>) format('embedded-opentype'),
       url(<%= font_url 'Diner-Obese-webfont.woff' %>) format('woff'),
       url(<%= font_url 'Diner-Obese-webfont.ttf' %>) format('truetype'),
       url(<%= font_url 'Diner-Obese-webfont.svg#diner-obese' %>) format('svg');
  font-weight: normal;
  font-style: normal;
}

