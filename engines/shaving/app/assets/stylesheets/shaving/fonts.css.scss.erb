@font-face {
  font-family: 'Diner';
  src: url(<%= font_url 'Diner-Fatt-webfont.eot' %>);
  src: url(<%= font_url 'Diner-Fatt-webfont.eot?#iefix' %>) format('embedded-opentype'),
       url(<%= font_url 'Diner-Fatt-webfont.woff' %>) format('woff'),
       url(<%= font_url 'Diner-Fatt-webfont.ttf' %>) format('truetype'),
       url(<%= font_url 'Diner-Fatt-webfont.svg#diner-fatt' %>) format('svg');
  font-weight: normal;
  font-style: normal;
}
